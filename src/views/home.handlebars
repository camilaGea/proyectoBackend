<div class="conteiner">
    <table id="tableProduct" >
        <thead>
            <tr>
            <th >ID</th>
            <th >Nombre</th>
            <th >Precio</th>
            <th >Codigo</th>
            <th >Stock</th>
            <th >Categoria</th>
            <th >Descripcion</th>
            <th >Status</th>
            </tr>
        </thead>
        {{#each productos}}
            <tbody>
                <tr>
                <td>{{this._id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.price}}</td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
                <td>{{this.description}}</td>
                <td>{{this.status}}</td>
                </tr>
            </tbody>
        {{/each}}
</div>